<div>
   <div class="bg-black px-4 py-3">
      <div class="d-flex justify-content-between align-items-center">
         <span class="h6 m-0 fw-800">BARANGAY</span>
         <a href="#formBrgy" class="i-plus" onclick="addBrgy()"><i class="bi bi-plus-circle-fill float-right"></i></a>
      </div>

      <div id="displayFormBrgy" style="display: none;">
         {{>form-brgy}}
      </div>
   </div>
   <div class="bg-gray px-4 pb-4 pt-2 mb-3">
      <table class="table table-hover table-striped table-sm m-0">
         <thead>
            <tr>
               <th scope="col">ID</th>
               <th scope="col">Barangay</th>
               <th scope="col" style="width: 100px">Actions</th>
            </tr>
         </thead>
         <tbody>
            {{#each parameter}}
            <tr>
               <td scope="row">{{this.BarangayID}}</td>
               <td>{{this.Name}}</td>
               <td>
                  <a href="/viewBrgy/{{this.BarangayID}}" type="button"><i class="bi bi-eye bi-fm"></i></a>
                  <a href="/editBrgy/{{this.BarangayID}}" type="button"><i class="bi bi-pencil bi-fm"></i></a>
                  <a href="/:{{this.BarangayID}}" type="button"><i class="bi bi-person-x bi-fm"></i></a>
               </td>
            </tr>
            {{/each}}
         </tbody>
      </table>
   </div>
</div>